{
  "[html,markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript,typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[scss,css,html]": {
    "editor.defaultFormatter": "stylelint.vscode-stylelint"
  },
  "stylelint.validate": ["scss", "css", "html"]
}
